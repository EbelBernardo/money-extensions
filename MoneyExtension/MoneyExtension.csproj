<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>MoneyExtensions_BeEbel</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Bernardo Ebel</Authors>
    <PackageDescription>Extension to convert decimal to integer</PackageDescription>
    <RepositoryUrl>https://github.com/EbelBernardo/money-extensions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/EbelBernardo/money-extensions</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>money;int;decimal;converter</PackageTags>
    <Title>MoneyExtensions_BeEbel</Title>
    <Summary>Extension to convert decimal to integer</Summary>

  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Media\icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
